{
	"type":"origins:multiple",
	"shouldclick":{
	   "type":"origins:resource",
	   "min":0,
	   "max":1,
	   "start_value":1,
	   "hud_render":{
		  "should_render":false
	   }
	},
	"duration":{
	   "type":"origins:resource",
	   "min":0,
	   "max":3,
	   "start_value":0,
	   "min_action":{
		  "type":"origins:and",
		  "actions":[
			 {
				"type":"origins:change_resource",
				"resource":"kin_of_sculk:keen_ears_noise_shouldclick",
				"change":1,
				"operation":"set"
			 },
			 {
				"type":"origins:if_else",
				"condition":{
				   "type":"origins:command",
				   "command":"execute if entity @e[distance=0.1..30,tag=kin_of_sculk]",
				   "comparison":"==",
				   "compare_to":0
				},
				"if_action":{
				   "type":"origins:revoke_power",
				   "power":"kin_of_sculk:keen_ears_noise",
				   "source":"kin_of_sculk:kin_of_sculk"
				}
			 }
		  ]
	   },
	   "max_action":{
		  "type":"origins:if_else",
		  "condition":{
			 "type":"origins:resource",
			 "resource":"kin_of_sculk:keen_ears_noise_shouldclick",
			 "comparison":"==",
			 "compare_to":1
		  },
		  "if_action":{
			 "type":"origins:and",
			 "actions":[
				{
				   "type":"origins:change_resource",
				   "resource":"kin_of_sculk:keen_ears_noise_shouldclick",
				   "change":0,
				   "operation":"set"
				},
				{
				   "type":"origins:execute_command",
				   "command":"execute as @a[distance=0.1..20,tag=kin_of_sculk] at @s run playsound kin_of_sculk:keen_ears.crackle_fast player @a ~ ~ ~ 0.20 1.00 0.20"
				}
			 ]
		  }
	   },
	   "hud_render":{
		  "should_render":false
	   }
	},
	"decay":{
	   "type":"origins:action_over_time",
	   "entity_action":{
		  "type":"origins:change_resource",
		  "resource":"kin_of_sculk:keen_ears_noise_duration",
		  "change":-1
	   },
	   "interval":20,
	   "condition":{
		  "type":"origins:or",
		  "conditions":[
			 {
				"type":"origins:moving",
				"inverted":true
			 },
			 {
				"type":"origins:sneaking"
			 },
			 {
				"type":"origins:on_block",
				"inverted":true
			 },
			 {
				"type":"origins:command",
				"command":"execute if entity @s[tag=hexblade]",
				"comparison":"==",
				"compare_to":1
			 }
		  ]
	   }
	},
	"moving":{
	   "type":"origins:action_over_time",
	   "entity_action":{
		  "type":"origins:change_resource",
		  "resource":"kin_of_sculk:keen_ears_noise_duration",
		  "change":3,
		  "operation":"set"
	   },
	   "interval":3,
	   "condition":{
		  "type":"origins:and",
		  "conditions":[
			 {
				"type":"origins:or",
				"conditions":[
				   {
					  "type":"origins:moving"
				   },
				   {
					  "type":"origins:sprinting"
				   }
				]
			 },
			 {
				"type":"origins:and",
				"conditions":[
				   {
					  "type":"origins:sneaking",
					  "inverted":true
				   },
				   {
					  "type":"origins:command",
					  "command":"execute if entity @s[tag=hexblade]",
					  "comparison":"==",
					  "compare_to":1,
					  "inverted":true
				   }
				]
			 },
			 {
				"type":"origins:on_block"
			 }
		  ]
	   }
	},
	"falling":{
	   "type":"origins:action_on_land",
	   "entity_action":{
		  "type":"origins:change_resource",
		  "resource":"kin_of_sculk:keen_ears_noise_duration",
		  "change":3,
		  "operation":"set"
	   },
	   "condition":{
		  "type":"origins:fall_distance",
		  "comparison":">",
		  "compare_to":4
	   }
	},
	"break":{
	   "type":"origins:action_on_block_break",
	   "entity_action":{
		  "type":"origins:change_resource",
		  "resource":"kin_of_sculk:keen_ears_noise_duration",
		  "change":3,
		  "operation":"set"
	   }
	},
	"blockuse":{
	   "type":"origins:action_on_block_use",
	   "entity_action":{
		  "type":"origins:change_resource",
		  "resource":"kin_of_sculk:keen_ears_noise_duration",
		  "change":3,
		  "operation":"set"
	   },
	   "block_condition":{
		  "type":"origins:or",
		  "conditions":[
			 {
				"type":"origins:block",
				"block":"minecraft:chest"
			 },
			 {
				"type":"origins:block",
				"block":"minecraft:ender_chest"
			 },
			 {
				"type":"origins:block",
				"block":"minecraft:trapped_chest"
			 },
			 {
				"type":"origins:block",
				"block":"minecraft:shulker_box"
			 }
		  ]
	   }
	},
	"itemuse":{
	   "type":"origins:action_on_item_use",
	   "entity_action":{
		  "type":"origins:change_resource",
		  "resource":"kin_of_sculk:keen_ears_noise_duration",
		  "change":3,
		  "operation":"set"
	   }
	},
	"whendamage":{
	   "type":"origins:action_when_damage_taken",
	   "entity_action":{
		  "type":"origins:change_resource",
		  "resource":"kin_of_sculk:keen_ears_noise_duration",
		  "change":3,
		  "operation":"set"
	   },
	   "cooldown":1
	}
}